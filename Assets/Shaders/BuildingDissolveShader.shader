				float temp_output_22_0 = ( (0.0 + (simpleNoise14 - _NoiseStrength_Instance) * (1.0 - 0.0) / (-_NoiseStrength_Instance - _NoiseStrength_Instance)) + _CutoffHeight_Instance );
				float temp_output_25_0 = step( WorldPosition.y , temp_output_22_0 );
				float4 _MossAlbedo_Instance = UNITY_ACCESS_INSTANCED_PROP(BuildingDissolveShader,_MossAlbedo);
				float _EdgeWidth_Instance = UNITY_ACCESS_INSTANCED_PROP(BuildingDissolveShader,_EdgeWidth);
